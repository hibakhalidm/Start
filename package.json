{
  "name": "cifad",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build:wasm": "cd src-wasm && cargo build --target wasm32-unknown-unknown --release --target-dir target_build -j 1 && wasm-bindgen target_build/wasm32-unknown-unknown/release/cifad_wasm.wasm --out-dir pkg --target web",
